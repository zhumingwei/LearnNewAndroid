<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeIncreaseView">
        <attr name="largeColor" format="color"/>
        <attr name="smallColor" format="color"/>
        <attr name="largeSize" format="dimension" />
        <attr name="smallSize" format="dimension" />
        <attr name="hour" format="integer"/>
        <attr name="min" format="integer"/>
        <attr name="wordspacing" format="dimension"/>
        <attr name="huddle" format="boolean"></attr>
        <attr name="textstyle" format="string"></attr>
        <attr name="needHour" format="boolean"></attr>
        <attr name="needMin" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="SmoothToggleButton">
